{"ast":null,"code":"import { ref } from 'vue';\nimport useDocument from '../composables/useDocument';\nexport default {\n  props: ['playlist'],\n\n  setup(props) {\n    const title = ref('');\n    const artist = ref('');\n    const showForm = ref(false);\n    const {\n      updateDoc\n    } = useDocument('playlists', props.playlist.id);\n\n    const handleSubmit = async () => {\n      const newSong = {\n        title: title.value,\n        artist: artist.value,\n        id: Math.floor(Math.random() * 1000000)\n      };\n      const res = await updateDoc({\n        songs: [...props.playlist.songs, newSong]\n      });\n      title.value = '';\n      artist.value = '';\n    };\n\n    return {\n      title,\n      artist,\n      showForm,\n      handleSubmit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAaA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,4BAAxB;AAEA,eAAe;AACbC,OAAK,EAAE,CAAC,UAAD,CADM;;AAEbC,OAAK,CAACD,KAAD,EAAQ;AACX,UAAME,KAAI,GAAIJ,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMK,MAAK,GAAIL,GAAG,CAAC,EAAD,CAAlB;AACA,UAAMM,QAAO,GAAIN,GAAG,CAAC,KAAD,CAApB;AACA,UAAM;AAAEO;AAAF,QAAgBN,WAAW,CAAC,WAAD,EAAcC,KAAK,CAACM,QAAN,CAAeC,EAA7B,CAAjC;;AAEA,UAAMC,YAAW,GAAI,YAAY;AAC/B,YAAMC,OAAM,GAAI;AACdP,aAAK,EAAEA,KAAK,CAACQ,KADC;AAEdP,cAAM,EAAEA,MAAM,CAACO,KAFD;AAGdH,UAAE,EAAEI,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,OAA3B;AAHU,OAAhB;AAKA,YAAMC,GAAE,GAAI,MAAMT,SAAS,CAAC;AAC1BU,aAAK,EAAE,CAAC,GAAGf,KAAK,CAACM,QAAN,CAAeS,KAAnB,EAA0BN,OAA1B;AADmB,OAAD,CAA3B;AAGAP,WAAK,CAACQ,KAAN,GAAc,EAAd;AACAP,YAAM,CAACO,KAAP,GAAe,EAAf;AACF,KAXA;;AAaA,WAAO;AAAER,WAAF;AAASC,YAAT;AAAiBC,cAAjB;AAA2BI;AAA3B,KAAP;AACF;;AAtBa,CAAf","names":["ref","useDocument","props","setup","title","artist","showForm","updateDoc","playlist","id","handleSubmit","newSong","value","Math","floor","random","res","songs"],"sourceRoot":"","sources":["/home/saumya/Documents/Project.Tech/project-tech/src/components/AddProject.vue"],"sourcesContent":["<template>\n  <div class=\"add-song\">\n    <!-- <button v-if=\"!showForm\" @click=\"showForm = true\">Add Songs</button> -->\n    <form v-if=\"showForm\" @submit.prevent=\"handleSubmit\">\n      <h4>Add a New Project</h4>\n      <input type=\"text\" placeholder=\"Project title\" required v-model=\"title\">\n      <input type=\"text\" placeholder=\"Project code base Link\" required v-model=\"artist\">\n      <button>Add</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport useDocument from '../composables/useDocument'\n\nexport default {\n  props: ['playlist'],\n  setup(props) {\n    const title = ref('')\n    const artist = ref('')\n    const showForm = ref(false)\n    const { updateDoc } = useDocument('playlists', props.playlist.id)\n\n    const handleSubmit = async () => {\n      const newSong = {\n        title: title.value,\n        artist: artist.value,\n        id: Math.floor(Math.random() * 1000000)\n      }\n      const res = await updateDoc({\n        songs: [...props.playlist.songs, newSong]\n      })\n      title.value = ''\n      artist.value = ''\n    }\n\n    return { title, artist, showForm, handleSubmit }\n  }\n}\n</script>\n\n<style scoped>\n  .add-song {\n    text-align: center;\n    margin-top: 40px;\n  }\n  form {\n    max-width: 100%;\n    text-align: left;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}